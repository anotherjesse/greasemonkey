<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#"> 
		   
  <RDF:Seq about="urn:mozilla:package:root">
    <RDF:li resource="urn:mozilla:package:greasemonkey"/>
  </RDF:Seq>
  
  <RDF:Description about="urn:mozilla:package:greasemonkey"
    chrome:displayName="Greasemonkey"
    chrome:author="Aaron Boodman and David Pullman"
    chrome:authorURL="http://youngpup.net, http://stilleye.net/"
    chrome:name="greasemonkey"
    chrome:extension="true"
    chrome:description="A user script manager for Firefox.">
  </RDF:Description>

  <RDF:Seq about="urn:mozilla:overlays">
    <RDF:li resource="chrome://browser/content/browser.xul"/>
    <RDF:li RDF:resource="chrome://navigator/content/navigatorOverlay.xul"/>
  </RDF:Seq>

  <RDF:Seq about="chrome://browser/content/browser.xul">
    <RDF:li>chrome://greasemonkey/content/browser.xul</RDF:li>
  </RDF:Seq>

  <RDF:Seq about="chrome://navigator/content/navigatorOverlay.xul">
    <RDF:li>chrome://greasemonkey/content/browser.xul</RDF:li>
  </RDF:Seq>

